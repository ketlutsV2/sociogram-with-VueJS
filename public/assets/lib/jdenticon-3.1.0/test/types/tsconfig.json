{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "typeRoots": [
            "../../node_modules/@types"
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "compileOnSave": false
}